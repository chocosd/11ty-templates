<div class="home-container dis-flex">
    <div class="header">
        <img src="{{ tem.logo | url }}">
    </div>
    
    <div class="menu-container dis-flex">

        <cc-open-module alt="{{ widget.title }}" class="widget dis-flex" id="{{ widget.link.id }}">
            <div class="icon-back dis-flex">
                <i class="material-icons">{{ widget.icon }}</i>
            </div>
            <span>{{widget.title}}</span>
        </cc-open-module>

    </div>
    
</div>